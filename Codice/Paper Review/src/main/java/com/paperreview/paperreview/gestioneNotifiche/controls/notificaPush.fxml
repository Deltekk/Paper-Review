<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.paperreview.paperreview.gestioneNotifiche.controls.NotificaPushControl"
            style="-fx-background-color: #333333dd; -fx-padding: 10; -fx-background-radius: 10;"
            prefWidth="300" prefHeight="100">
    <VBox spacing="5">
        <Label fx:id="titleLabel" style="-fx-text-fill: white; -fx-font-weight: bold;"/>
        <Label fx:id="messageLabel" style="-fx-text-fill: white;"/>
    </VBox>
</AnchorPane>